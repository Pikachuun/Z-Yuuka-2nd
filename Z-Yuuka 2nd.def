params = var(10), var(11), var(12), sysvar(1), var(2)
ignorehitpause = 1
persistent = 256

[State -2, Erase Helpers]
type = SelfState
trigger1 = (var(1) := 4) || 1
value = 11223343
ignorehitpause = 1
persistent = 256